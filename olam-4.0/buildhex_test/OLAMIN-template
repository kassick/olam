&OLAMIN

 !--------------------------------------------------------------------------
 ! Simulation title (64 chars)
 !--------------------------------------------------------------------------

   NL%EXPNME = 'OLAM test simulation'   

 !--------------------------------------------------------------------------
 ! Runtype: either MAKESFC, MAKEGRID, INITIAL, HISTORY, or PLOTONLY
 !--------------------------------------------------------------------------

 ! NL%RUNTYPE  = 'MAKESFC'
 ! NL%RUNTYPE  = 'MAKEGRID'
   NL%RUNTYPE  = 'INITIAL'
 ! NL%RUNTYPE  = 'HISTORY'
 ! NL%RUNTYPE  = 'PLOTONLY'
 ! NL%RUNTYPE  = 'PARCOMBINE'

 !--------------------------------------------------------------------------
 ! Simulation ending time
 !--------------------------------------------------------------------------

   NL%TIMEUNIT = 'h'      ! Time units of TIMMAX: 's','m','h','d'
   NL%TIMMAX   = 2.       ! Ending time of this model run
 
 !--------------------------------------------------------------------------
 ! Start of simulation or ISAN processing (when model time counter TIME8 = 0.)
 !--------------------------------------------------------------------------

   NL%ITIME1   = 0000     ! UTC
   NL%IDATE1   = 1        ! Day
   NL%IMONTH1  = 5        ! Month
   NL%IYEAR1   = 1997     ! Year

 !--------------------------------------------------------------------------
 ! Vertical grid specifications
 !--------------------------------------------------------------------------

   NL%NZP  = 100  ! # of vertical levels [or max # if limited by HDZ(NDZ)]

   NL%NDZ  = 4    ! # of (HDZ,DZ) pairs of values to use from list (min=2, max=10)
                  ! Exception: NDZ = 1 is flag to use ZZ instead of (HDZ,DZ) pairs

   NL%HDZ  =    0.,  1000., 15000., 35000.  ! Heights at which DZ is specified
   NL%DZ   =  100.,   200.,  2000.,  2000.  ! Vertical grid spacing at each HDZ

   ! Vertical levels if NDZ = 1
   NL%ZZ       = 0.0,
     30.0,      60.0,      90.0,     120.0,     150.0,
    180.0,     210.0,     240.0,     270.0,     300.0,
    330.0,     360.0,     390.0,     420.0,     450.0,
    480.0,     510.0,     540.0,     570.0,     600.0,
    630.0,     660.0,     690.0,     720.0,     750.0,
    780.0,     810.0,     840.0,     870.0,     900.0,
    930.0,     960.0,     990.0,    1020.0,    1050.0, 
   1080.0,    1110.0,    1140.0,    1170.0,    1200.0,
   1230.0,    1260.0,    1290.0,    1320.0,    1350.0, 
   1380.0,    1410.0,    1440.0,    1470.0,    1500.0,
   1533.0,    1569.3,    1609.2,    1653.2,    1701.5,
   1754.6,    1813.1,    1877.4,    1948.1,    2025.9,
   2111.5,    2205.7,    2309.3

 !--------------------------------------------------------------------------
 ! Horizontal grid specifications
 !--------------------------------------------------------------------------

   NL%MDOMAIN = 0       ! Model domain specification:
                        !    0 = global spherical
                        !    3 = 2D Cartesian channel with cyclic boundaries

   NL%MESHTYPE = 2      ! Mesh type 1 = Delaunay (triangles)
                        !           2 = Voronoi  (hexagons)

   NL%NXP  = 20         ! Number of grid points spanning one side of
                        !  any icosahedral triangle when MDOMAIN = 0 
                        ! or spanning X direction when MDOMAIN = 3

   NL%DELTAX  = 1000.   ! Coarse grid horizontal spacing (used if MDOMAIN > 0)

 !--------------------------------------------------------------------------
 ! Horizontal grid refinement ("nested grid") definition
 !--------------------------------------------------------------------------

   ! Number of grids to run:
   NL%NGRIDS  = 2       

   ! Method to reduce grid concavities when generating each mesh
   ! 1 eliminates only sharp concavities (Method B)
   ! 2 eliminates both sharp and gentler concavities (original Method A)
   ! 3 activates the Method C refined mesh algorithm
   ! The first value (for the base grid) is ignored
   NL%NCONCAVE = 1,1,1,1,1,

   ! Number of transition rows when creating finer mesh. Only used for 
   ! grids with NCONCAVE = 2. The 1st value is ignored. Valid MROWS = 1:5
   NL%MROWS = 3,3,3,3,3, 

   ! 1 allows all model points to move when generating mesh
   ! 0 allows only the transition rows to move generating mesh
   ! The first value is ignored, and only used for grids with NCONCAVE=3
   NL%MOVEALL = 1,1,1,1,1,

 ! One group of values must be provided for each "grid" number NG = 2:NGRIDS

 ! NGRDLL(NG) = Number of geographic points that you use to define the extent
 !              of grid NG
 ! GRDRAD(NG) = Radius of influence for grid NG.  Any grid point closer than
 !              this radius to any of the geographic points or to the line
 !              segments that connect consecutive points in the sequence will
 !              be included in the refined area.
 ! GRDLAT(NG,:) = latitude/longitude of all geographic points in the
 ! GRDLON(NG,:)   sequence for grid NG

   ! Grid 2:
   NL%NGRDLL(2)     =  2
   NL%GRDRAD(2)     =  2000.e3
   NL%GRDLAT(2,1:2) =  41.0,  36.0,
   NL%GRDLON(2,1:2) = -83.0, -79.0,

   ! Grid 3:
   NL%NGRDLL(3)     =  1
   NL%GRDRAD(3)     =  1500.e3
   NL%GRDLAT(3,1:1) =  41.0,
   NL%GRDLON(3,1:1) = -83.0,

   ! Grid 4:
   NL%NGRDLL(4)     =  1
   NL%GRDRAD(4)     =  1000.e3
   NL%GRDLAT(4,1:1) =  41.0,
   NL%GRDLON(4,1:1) = -83.0,

   ! Grid 5:
   NL%NGRDLL(5)     =  1
   NL%GRDRAD(5)     =  500.e3
   NL%GRDLAT(5,1:1) =  41.0,
   NL%GRDLON(5,1:1) = -83.0,

 !------------------------------------------------------------------------------
 ! Timestep and timestep ratios (One ratio for each Mesh Refinement Level (MRL))
 !------------------------------------------------------------------------------

   NL%DTLONG  = 30.      ! Coarse grid long timestep

   NL%NDTRAT   = 1,1,1,1,1       ! # long MRL timesteps per long MRL-1 timestep
   NL%NACOUST  = 1,1,1,1,1       ! # acoustic timesteps per long timestep


 !--------------------------------------------------------------------------
 ! Variable initialization input
 !--------------------------------------------------------------------------

   NL%INITIAL  = 3   ! Initial fields:  1 = horizontally homogeneous
                     !                  2 = 3D heterogeneous
                     !                  3 = longitudinally homogeneous

   ! input climate file:
   NL%ZONCLIM  = '../etc/ZONAVG_CLIMATE'

 !--------------------------------------------------------------------------
 ! Nudging parameters (used only if initial=2)
 !--------------------------------------------------------------------------

   NL%NUDFLAG = 0        ! 4DDA nudging flag: 0 = no nudging
                         !                    1 = nudge from isan files

   NL%NUDNXP = 0         ! NXP value for defining nudging grid

   NL%TNUDCENT = 21600.   ! 4DDA nudging time scale (s)

 !--------------------------------------------------------------------------
 ! Grid file path/name
 !--------------------------------------------------------------------------

   NL%GRIDFILE = 'sfcfiles/gridfile_1.h5'

 !--------------------------------------------------------------------------
 ! History file I/O
 !--------------------------------------------------------------------------

   NL%IOUTPUT   = 1            ! 0-no files, 1-write files

   NL%HFILEPREF = 'hist/b'     ! History file output path/prefix
   NL%ICLOBBER  = 1            ! 0 = stop if files exist, 1 = overwite files
   NL%ICOMPRESS = 3            ! 0 (no compres, fast) <-> 9 (max compres, slow)
   NL%FRQSTATE  = 3600.        ! Time interval between history files

   NL%HFILIN   = 'hist/b-H-2004-09-01-030000.h5'      ! History file intput

 !--------------------------------------------------------------------------
 ! Topography initialization
 !--------------------------------------------------------------------------

   NL%ITOPOFLG = 1    ! 1 = Read topo from standard TOPO_DATABASE file
                      ! 2 = Define topo in subroutine topo_init

 ! Surface topography database file path and name:
   NL%TOPO_DATABASE = './data/dem30s/TM'

 !--------------------------------------------------------------------------
 ! Model options / Numerical schemes
 !--------------------------------------------------------------------------

   NL%NADDSC    = 0       ! Number of additional scalar species

   NL%ICORFLG   = 1       ! Coriolis flag/2D v-component: 0 = off, 1 = on

   NL%DEBUG_FP  = .false. ! Detect floating point errors such as overflows,
                          ! INFs, divide-by-zeros, and NANs (best if code is
                          ! compiled with debug options too)

   NL%INIT_NANS = .false. ! Initialize model variables to NANs to catch
                          ! errors (useful if DEBUG_FP is set to .true.)

 !--------------------------------------------------------------------------
 ! Rayleigh friction parameters
 !--------------------------------------------------------------------------

   NL%RAYF_ZMIN   = 25000.  ! Damping for U, THIL - minimum height (m)
   NL%RAYF_DISTIM = 0.0     ! Damping for U, THIL - model top disp time (s)
   NL%RAYF_EXPON  = 1.0     ! Damping for U, THIL - exponent for profile
   
   NL%RAYFW_ZMIN   = 25000. ! Damping for W - minimum height (m)
   NL%RAYFW_DISTIM = 0.0    ! Damping for W - model top disp time
   NL%RAYFW_EXPON  = 1.0    ! Damping for W - exponent for profile

 !--------------------------------------------------------------------------
 ! Radiation parameters
 !--------------------------------------------------------------------------
 
   NL%ISWRTYP = 3   ! Shortwave radiation type:
   NL%ILWRTYP = 3   ! Longwave  radiation type:
                       !   0=none, 1=Chen, 2=Mahrer/Pielke, 3=Harrington
   NL%RADFRQ  = 1800.  ! Time interval between radiation tendency updates (s)

 !--------------------------------------------------------------------------
 ! Cumulus parameterization parameters
 !--------------------------------------------------------------------------
  
   NL%NQPARM = 2,2,2,2,2,2,2,2   ! Cumulus parameterization scheme for each MRL
                                 ! 0=off, 1=Kuo, 2=Grell

   NL%NQPARM_SH = 0,0,0,0,0,0,0,0  ! Shallow cumulus parameterization scheme 
                                   ! for each MRL:  0=off, 1=Grell

   NL%CONFRQ = 1200.   ! Time interval (s) between cumulus param updates

   NL%WCLDBS = .002     ! Minimum vertical motion (m/s) needed at cloud base 
                       ! to trigger convection in Kuo scheme
 
 !--------------------------------------------------------------------------
 ! Eddy diffusion coefficient parameters (one value for each MRL)
 !--------------------------------------------------------------------------
 
   NL%IDIFFK = 2,2,2,2,2          ! Turbulent parameterization scheme:
                                  !  0 = off
                                  !  2 = Smagorinsky-Lilly-Hill
                                  !  7 = Taylor

   NL%CSX    = .2,.2,.2,.2,.2     ! Horizontal length scale coefficient
   NL%CSZ    = .2,.2,.2,.2,.2     ! Vertical length scale coefficient
   NL%XKHKM  = 3.,3.,3.,3.,3.     ! Ratio of horiz K_h to K_m 
   NL%ZKHKM  = 3.,3.,3.,3.,3.     ! Ratio of vert K_h to K_m
   NL%AKMIN  = 0.,0.,0.,0.,0.     ! Ratio of minimum horizontal eddy
                                  !  viscosity coefficient to typical value
                                  !  from deformation K

 !--------------------------------------------------------------------------
 ! Microphysics
 !--------------------------------------------------------------------------

   NL%LEVEL   = 3  ! Moisture complexity level:
                   !  0 = no moisture
                   !  1 = vapor only
                   !  2 = vapor and diagnosed cloud only
                   !  3 = use bulk microphysics 
 
                   ! Choices:
                   !-----------!
   NL%ICLOUD  = 4  ! 0,1,4,5,6,7 ! 0 = category off
   NL%IDRIZ   = 4  ! 0,1,4,5,6,7 ! 1 for ICLOUD, IDRIZ = same as 4 but CPARM, 
   NL%IRAIN   = 2  ! 0,1,2,5     !   DPARM are not used
   NL%IPRIS   = 5  ! 0,5,6,7     ! 1 for IRAIN, ISNOW, IAGGR, IGRAUP, IHAIL 
   NL%ISNOW   = 2  ! 0,1,2,5     !   = same as 2 but RPARM, SPARM, APARM, 
   NL%IAGGR   = 2  ! 0,1,2,5     !     GPARM, HPARM not used
   NL%IGRAUP  = 2  ! 0,1,2,5     ! 2 = diameter specified and concen diagnosed
   NL%IHAIL   = 2  ! 0,1,2,5     ! 4 = concen specified and diameter diagnosed
                                 ! 5,6,7 = concen prognosed and diameter diagnosed
                           ! 5 = CCN, GCCN, IFN concen specified constant
                           ! 6 = CCN, GCCN, IFN concen specified in vert profile
                           ! 7 = CCN, GCCN, IFN prognosed (incomplete)
                                
   NL%ICCNLEV = 0,        ! CCN 0=no source or sink
                          !     1=nucleation CCN sink, evaporation CCN source 

   NL%CPARM   = 300.e6    ! Cloud concen (#/kg_air) if ICLOUD = 4,
                          ! CCN concen (#/kg_air) if ICLOUD = 5

   NL%DPARM   = 10.       ! Drizzle concen ([#/kg_air) if ICLOUD = 4,
                          ! GCCN concen (#/kg_air) if ICLOUD = 5

   NL%RPARM   = 1.0e-3    ! Rain droplet mean diameter (m) if IRAIN = 2
   NL%PPARM   = 0.0       ! Not used
   NL%SPARM   = 1.0e-3    ! Snow mean diameter (m) if ISNOW = 2
   NL%APARM   = 1.0e-3    ! Aggregates mean diameter (m) if IAGGR = 2
   NL%GPARM   = 1.0e-3    ! Graupel mean diameter (m) if IGRAUP = 2
   NL%HPARM   = 3.0e-3    ! Hail mean diameter (m) if IHAIL = 2

   NL%CNPARM  = .04e-6    ! Median radius of the CCN dist (m)
   NL%GNPARM  = 3.0e-6    ! Median radius of the GCCN dist (m)

 !--------------------------------------------------------------------------
 ! Sounding specification
 !--------------------------------------------------------------------------

   NL%NSNDG   = 24      ! Number of sounding levels to be read below
 
   NL%IPSFLG  = 1       ! Specifies what is in PS array:
                        !    0 = pressure(mb) 
                        !    1 = heights(m MSL)
 
   NL%ITSFLG  = 0       ! Specifies what is in TS array:
                        !    0 = temp(C)
                        !    1 = temp(K)
                        !    2 = pot. temp(K)

   NL%IRTSFLG = 3       ! Specifies what is in RTS array:
                        !    0 = dew pnt.(C)
                        !    1 = dew pnt.(K) 
                        !    2 = mix rat(g/kg) 
                        !    3 = relative humidity in %
                        !    4 = dew pnt depression(K)
 
   NL%IUSFLG  = 0       ! Specifies what is in US and VS arrays:
                        !    0 = u,v component(m/s) 
                        !    1 = umoms-direction, vmoms-speed
 
   NL%HS      = 0.0     ! Height (m MSL) of lowest sounding level
                        ! Only used for IPSFLG=0 (if PS array is pressure)

   NL%P_SFC   = 1010.0  ! Surface Pressure (mb)
                        ! Only used for IPSFLG=1 (if PS array is height)

 !--------------------------------------------------------------------------
 ! Sounding:      PS      TS      RTS     US     VS
 !--------------------------------------------------------------------------
   NL%SOUNDING = 0.0,   25.0,   10.0,   0.0,   0.0,
              1000.0,   16.0,   10.0,   0.0,   0.0,
              2000.0,    7.0,   10.0,   0.0,   0.0,
              3000.0,   -2.0,   10.0,   0.0,   0.0,
              4000.0,  -11.0,   10.0,   0.0,   0.0,
